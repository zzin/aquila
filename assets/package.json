{
	"name": "assets",
	"version": "1.0.0",
	"description": "Aquila theme packages",
	"main": "main.js",
	"scripts": {
		"prod": "cross-env NODE_ENV=production webpack --mode production --progress",
		"dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"clean": "rm -rf build/*",
		"svg": "svgr -d src/js/icons src/icons",
		"lint": "eslint src/js/*",
		"lint:fix": "eslint --fix src/js*",
		"lint-css": "stylelint 'src/sass/**/*.scss'",
		"lint-css:fix": "npm run lint-css -- --fix",
		"precommit": "npm run lint-css:fix && npm run lint:fix && npm run build"
	},
	"keywords": [
		"wordpress",
		"themes"
	],
	"author": "Imran Sayed",
	"license": "MIT",
	"private": true,
	"browserslist": [
		"defaults"
	],
	"devDependencies": {
		"@babel/core": "^7.21.5",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-react": "^7.18.6",
		"@prettier/plugin-php": "^0.19.4",
		"@svgr/cli": "^7.0.0",
		"@wordpress/base-styles": "^4.23.0",
		"@wordpress/block-editor": "^12.0.0",
		"@wordpress/blocks": "^12.9.0",
		"@wordpress/components": "^23.9.0",
		"@wordpress/compose": "^6.9.0",
		"@wordpress/data": "^9.2.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^4.15.0",
		"@wordpress/element": "^5.9.0",
		"@wordpress/eslint-plugin": "^14.5.0",
		"@wordpress/hooks": "^3.32.0",
		"@wordpress/i18n": "^4.32.0",
		"@wordpress/icons": "^9.23.0",
		"@wordpress/server-side-render": "^4.9.0",
		"babel-loader": "^9.1.2",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"cssnano": "^6.0.1",
		"eslint": "^8.39.0",
		"eslint-plugin-jsdoc": "^43.1.1",
		"eslint-webpack-plugin": "^4.0.1",
		"file-loader": "^6.2.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^2.7.5",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"sass-mq": "^6.0.0",
		"style-loader": "^3.3.2",
		"stylelint": "^13.13.1",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-webpack-plugin": "^4.1.1",
		"terser-webpack-plugin": "^5.3.7",
		"url-loader": "^4.1.1",
		"webpack": "^5.81.0",
		"webpack-cli": "^5.0.2"
	}
}
